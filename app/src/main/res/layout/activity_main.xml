<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="com.example.baiwen.vcloset.MainActivity">

    <android.support.design.widget.AppBarLayout
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay"
        android:background="@android:color/darker_gray">

    </android.support.design.widget.AppBarLayout>


    <!--<Button-->
        <!--android:text="Avatar"-->
        <!--android:layout_height="78dp"-->
        <!--android:id="@+id/button2"-->
        <!--android:layout_gravity="bottom|right"-->
        <!--android:backgroundTint="@android:color/holo_blue_dark"-->
        <!--android:layout_width="195dp" />-->


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton5"
        android:layout_margin="@dimen/fab_margin"
        app:srcCompat="@drawable/blackfileicon"
        android:layout_gravity="top|left"
        app:backgroundTint="@android:color/transparent"
        android:layout_width="69dp"
        android:layout_height="72dp"
        app:rippleColor="?android:attr/colorBackgroundCacheHint" />


    <Button
        android:text="Outfit"
        android:id="@+id/button"
        android:layout_gravity="bottom|left"
        android:layout_height="65dp"
        android:layout_width="128dp"
        android:theme="@style/MyButton"
        android:backgroundTint="@color/white"
        android:layout_margin="@dimen/fab_margin"
        android:elevation="0dp"
        style="@style/MyButton" />

    <Button
        android:text="Avatar"
        android:layout_height="65dp"
        android:id="@+id/button2"
        android:layout_margin="@dimen/fab_margin"
        android:layout_gravity="bottom|right"
        android:backgroundTint="@color/white"
        android:layout_width="128dp"
        android:theme="@style/MyButton"
        style="@style/MyButton" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton6"
        android:layout_width="69dp"
        android:layout_margin="@dimen/fab_margin"
        app:srcCompat="@drawable/blackcamaraicon"
        android:layout_gravity="top|right"
        app:backgroundTint="@android:color/transparent"
        android:layout_height="71dp" />

    <android.support.v4.view.ViewPager
        android:id="@+id/pagerMain"
        android:layout_width="wrap_content"
        android:layout_height="246dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_gravity="center">
    </android.support.v4.view.ViewPager>


</android.support.design.widget.CoordinatorLayout>
